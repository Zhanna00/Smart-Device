{% extends '../common/layout.twig' %}

{% block main %}
	<main>
		<section>
			<div class="container">
				<h1>Разводящая страница</h1>
				<ol>
					{% for page in pages %}
						<li>
							<a href="{{ page.id }}.html">
								{{ page.title }}
								—
								{{ page.id }}.html
							</a>
						</li>
					{% endfor %}
				</ol>
			</div>
		</section>
	</main>
{% endblock %}
